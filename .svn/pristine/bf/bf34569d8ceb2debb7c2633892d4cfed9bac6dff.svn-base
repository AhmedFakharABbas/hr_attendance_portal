.ac-title {
  background-image: linear-gradient(#F7F7F7 , #D6D6D6);
  /* background: url("../../../../assets/img/box-title.gif") repeat left top; */
  padding: 8px 10px 8px 10px;
  font: 13px/1em Verdana, Arial, Tahome ;
  color: #343434;
  font-weight: bold;
}
.ac-title-with-input {
  background-image: linear-gradient(#F7F7F7 , #D6D6D6);
  /* background: url("../../../../assets/img/box-title.gif") repeat left top; */
  padding: 8px 10px;
  font: 13px/1em Verdana, Arial, Tahome;
  color: #343434;
  font-weight: bold;
}
.ml-20{
  margin-left: 20px;

}
.mr-20{
  margin-right: 20px;

}
.ac-content {
  background: url("../../../../assets/img/box-shadow.gif") repeat-x left top;
  padding: 5px;
}
.ac-table {
  width: 100%;
  overflow: auto;
  max-height: 62vh;
}
.ac-box {
  display: block;
  background: #fcfcfc !important;
  border: 1px solid #e7e7e7;
  flex-direction: row;
}
.ac-title-withoutpic {
  font: 12px/1em Verdana, Arial, Tahome;
  color: #333333;
  font-weight: 600;
}
.grayborder {
  border: 1px solid #e7e7e7;
  border-radius: 5px;
}
textarea {
  border-bottom: none;
  border-left: none;
  border-right: none;
  border-top: 1px solid #e7e7e7;
  outline: none;
}
.ac-footer {
  background: #D8DCDF;
  padding: 2px 10px 4px;
}
#ac-button-list {
  margin: 3px 0;
  text-align: right;
}

#ac-button-list a,
.ac-button-list a,
#ac-button-list font,
.ac-button-list font,
#ac-button,
a#ac-button,
a.ac-button {
  position: relative;
  padding: 4px 6px 4px 6px;
  border: 1px solid var(--unnamed-color-ffffff);
  background: transparent linear-gradient(180deg, #ededed 0%, #fafafa 100%) 0%
    0% no-repeat padding-box;
  border: 1px solid #ffffff;
  border-radius: 3px;
  color: #3080ea;
  opacity: 1;
  text-decoration: none;
  font-size: 12px;
}

#ac-button-list a:hover,
.ac-button-list a:hover,
#ac-button a:hover,
.ac-button a:hover,
a.ac-button:hover,
a#ac-button:hover {
  border: 1px solid #a6daee;
  padding: 4px 6px 4px 6px;
  background: transparent linear-gradient(0deg, #f1fafe 0%, #bde9fb 100%) 0% 0%
    no-repeat padding-box;
  border-radius: 3px;
  opacity: 1;
  color: #343434;
  text-decoration: none;
  font-size: 12px;
}
.PendingStatus {
   color: #ef9400 !important;
   height: 28px;

   justify-content: left !important;
}
.employeeName {
  color: #3080EA !important;;
}
.pending {

  color: #ef9400 !important;
  height: 28px !important;;
  justify-content: left !important;

}
button[disabled] {
  color: #8d8d8d !important;
}
.approved {
  color: #61bf63 !important;
  height: 28px !important;
  justify-content: left !important;

}
.custom-cell {
  color: #343434 !important;
  height: 28px !important;
  justify-content: left !important;
}
.custom-header{
  color: #3080ea !important;

  font-size: 12px !important;
  line-height: 1.3;
  font-weight: normal !important;
  height: 28px !important;
  justify-content: left !important;

}
.disapproved {
  color: #e84f2d !important;
  height: 28px !important;
  justify-content: left !important;

}


mat-table {
  cursor: pointer;
}

::ng-deep .mat-sort-header-arrow {
  margin: 0 0 0 0px !important;
}
.mat-header-cell {
  color: #3080EA, !important;
  font-family: Arial;
  font-size: 12px ;
  line-height: 1.3;
}
::ng-deepmat-cell:first-of-type, mat-header-cell:first-of-type, mat-footer-cell:first-of-type {
  padding-left: 20px;
}
.ml-10 {
  margin-left: 10px;
}
.mr-10 {
  margin-right: 10px;
}
.card {
  border: 1px solid white;
}
.mb-10{
  margin-bottom: 10px;
}
.right-content{

  /* position: relative;
  bottom: 5px; */

}
