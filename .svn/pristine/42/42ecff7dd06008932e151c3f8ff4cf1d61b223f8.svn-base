.box-shadow {
  box-shadow: 0px 3px 6px 0px rgb(0 0 0 / 16%);
}

@-webkit-keyframes fadein {
  from {
    bottom: 0;
    opacity: 0;
  }
  to {
    bottom: 30px;
    opacity: 1;
  }
}

@keyframes fadein {
  from {
    bottom: 0;
    opacity: 0;
  }
  to {
    bottom: 30px;
    opacity: 1;
  }
}

@-webkit-keyframes fadeout {
  from {
    bottom: 30px;
    opacity: 1;
  }
  to {
    bottom: 0;
    opacity: 0;
  }
}

@keyframes fadeout {
  from {
    bottom: 30px;
    opacity: 1;
  }
  to {
    bottom: 0;
    opacity: 0;
  }
}
.ac-title .fr.atend .fas.fa-caret-up:active {
  transform: rotate(180deg);
}
div.ac-title.collapsed[aria-expanded="false"] > .fr.atend > img {
  transform: rotate(180deg);
}
div.ac-title[aria-expanded="false"] > .fr > img {
  transform: rotate(180deg);
}
div.lm-title.collapsed[aria-expanded="false"] > .fr.atend > img {
  transform: rotate(180deg);
}

.mat-error-msg {
  position: absolute;
  font-size: 10px;
  right: 0;
  color: #fb887a;
  top: 23px;
}
.lm-box {
  border-left: 1px solid #e6e6e6;
  border-right: 1px solid #e6e6e6;
  border-bottom: 1px solid #e6e6e6;
  margin: 0px 10px 10px 10px;
  border-radius: 2px 2px 0px 0px;
  box-shadow: 0px 3px 6px 0px RGB(0 0 0 /16%);
}
::ng-deep .mat-form-field-appearance-outline .mat-form-field-outline {
  color: #d8dcdf !important;
}

::ng-deep .mat-form-field-appearance-outline .mat-form-field-outline-end,
::ng-deep .mat-form-field-appearance-outline .mat-form-field-outline-start {
  border-width: 1px !important;
  color: #d8dcdf !important;
}
::ng-deep mat-form-field .mat-form-field-label {
  color: #3080ea !important;
  font-size: 11px !important;
  font-weight: normal !important;
}
::ng-deep input.mat-input-element {
  margin-bottom: 2px;
  font-size: 11px;
  top: -4px;
  position: relative;
}
/* ::ng-deep .form-field-outline-thick .mat-form-field-outline-gap {
  border-width: 1px !important;
} */
::ng-deep.mat-form-field-appearance-outline
  .mat-form-field-outline-thick
  .mat-form-field-outline-gap {
  border-width: 1px !important;
}
::ng-deep.mat-option {
  height: 28px !important;
  padding-left: 10px !important;
  font-size: 11px !important;
  font-weight: 400 !important;
}
::ng-deep .mat-form-field-appearance-outline.mat-form-field-disabled .mat-form-field-label {
  color: #8d8d8d !important;

}
::ng-deep.mat-form-field.mat-form-field-invalid .mat-form-field-label {
  color: #3080ea !important;
  font-size: 11px;
  font-weight: normal;
}
/* ::ng-deep.mat-form-field .mat-select.mat-select-invalid .mat-select-arrow {
  color: #343434 !important;
  position: relative !important;
  top: 0px !important;
} */
::ng-deep.mat-select-trigger {
  display: inline-table;
  color: #343434 !important;
  cursor: pointer;
  position: relative;
  box-sizing: border-box;
  top: -6px;
  font-size: 11px;
}
::ng-deep.mat-select-arrow-wrapper {
  color: #343434 !important;
  display: table-cell;
  vertical-align: middle;
  position: relative;
  /* bottom: 6px; */
}
/* ::ng-deep.mat-form-field-appearance-outline .mat-form-field-flex {
  padding: 0px !important;
  margin-top: -0.25em;
  position: relative;
} */
::ng-deep.mat-select-arrow {
  color: #343434 !important;
}
::ng-deep.mat-form-field .mat-select.mat-select-invalid .mat-select-arrow {
  color: #343434 !important;
}
::ng-deep.mat-form-field.mat-focused.mat-primary .mat-select-arrow {
  color: #343434 !important;
}
::ng-deep.mat-form-field-flex {
  height: 26px;
}
::ng-deep.mat-form-field-appearance-outline .mat-form-field-flex {
  padding: 0px 0px 0px 8px;
  margin-top: -0.25em;
  position: relative;
}
.font-image {
  height: 13px;
  position: relative;
  bottom: 1px;
  left: 10px;
}
.text-area {
  resize: none;
  position: relative;
  bottom: 8px;
  font-size: 11px;
  /* white-space: nowrap; */
  border: none;
}

input[readonly]
textarea[readonly],
input[readonly]
  ::ng-deep
  ~ span.mat-form-field-label-wrapper
  label.mat-form-field-label,
textarea[readonly]
  ::ng-deep
  ~ span.mat-form-field-label-wrapper
  label.mat-form-field-label,
button[disabled] {
  color: #8d8d8d !important;
}
::ng-deep.mat-select-disabled .mat-select-value {
  color: #333333;
}
