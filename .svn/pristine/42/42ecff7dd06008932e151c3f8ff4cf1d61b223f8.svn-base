}

#ac-button-list a:hover,
.ac-button-list a:hover,
#ac-button a:hover,
.ac-button a:hover,
a.ac-button:hover,
a#ac-button:hover {
  border: 0px solid #a6daee;
  padding: 2px 6px 2px 6px;
  background: transparent linear-gradient(0deg, #f1fafe 0%, #bde9fb 100%) 0% 0%
    no-repeat padding-box;
  border-radius: 3px;
  opacity: 1;
  color: #343434;
  text-decoration: none;
  font-size: 11px;
}
.submitButton {
  color: #3080ea;
  position: relative;
  padding: 2px 6px 2px 6px;
  /* border: px solid var(--unnamed-color-ffffff);  */
  background: transparent linear-gradient(180deg, #ededed 0%, #fafafa 100%) 0%
    0% no-repeat padding-box;
  border: 0px solid #ffffff;
  border-radius: 3px;

  opacity: 1;
  text-decoration: none;
  font-size: 11px;
  cursor: pointer;
}
.submitButton:hover {
  /* border: 1px solid #a6daee; */
  padding: 2px 6px 2px 6px;
  background: transparent linear-gradient(0deg, #f1fafe 0%, #bde9fb 100%) 0% 0%
    no-repeat padding-box;
  border-radius: 3px;
  opacity: 1;
  color: #343434;
  text-decoration: none;
  font-size: 11px;
}
/* time picker

*/
input[type="time"]::-webkit-calendar-picker-indicator {
  width: 13px;
  height: 20px;
  padding-top: 0px;
  padding-bottom: 0px;
  filter: invert(15%) sepia(0%) saturate(0%) hue-rotate(346deg) brightness(92%)
    contrast(83%);
}
/* input type file section*/
input[type="file"] {
  display: none;
}
.custom-file-upload {
  cursor: pointer;
  margin-bottom: 0px !important;
  margin-right: 10px !important;
  color: #3080ea;
  position: relative;
  padding: 2px 6px 2px 6px;
  /* border: 1px solid var(--unnamed-color-ffffff); */
  background: transparent linear-gradient(180deg, #ededed 0%, #fafafa 100%) 0%
    0% no-repeat padding-box;
  border: 0px solid #ffffff;
  border-radius: 3px;
  opacity: 1;
  text-decoration: none;
  font-size: 11px;
}
.custom-file-upload:hover {
  /* border: 1px solid #a6daee; */
  padding: 2px 6px 2px 6px;
  background: transparent linear-gradient(0deg, #f1fafe 0%, #bde9fb 100%) 0% 0%
    no-repeat padding-box;
  border-radius: 3px;
  opacity: 1;
  color: #343434;
  text-decoration: none;
  font-size: 11px;
}
