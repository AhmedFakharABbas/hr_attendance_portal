/* ================ Attendance Correction */
.ac-box {
  margin-left: 20px !important;
  margin-right: 20px !important;
}
.ac-container {
  position: relative;
  clear: both;
  display: flex;
  background-color: #ffffff;
  margin-bottom: 10px;
}
.ac-form-container {
  position: relative;
  clear: both;
  display: flex;
  background-color: #ffffff;
  /* min-height: 96vh; */
}
.ac-left-container {
  width: 820px;
  float: left;
  background-color: #ffffff;
}
.ac-right-container {
  position: absolute;
  right: 0;
  top: 0;
  height: 100%;
  margin-right: 20px;

  width: 380px;
  float: right;
  background-color: white;
}
.ac-right-box {
  display: block;
  flex-direction: row-reverse;
  margin-top: 0% !important;
  background: #fcfcfc !important;
  border: 1px solid #e7e7e7;
  max-height: 100%;
  /* overflow-y: scroll; */
}
.ac-right-box-bottom {
  display: block;
  flex-direction: row-reverse;
  margin-top: 0% !important;
  background: #fcfcfc !important;
}
.ac-right-box_upper_contianer {
  height: 73vh;
}
.ac-title {
  background-image: linear-gradient(#f7f7f7, #d6d6d6);
  /* background: url('../../../../../assets/img/box-title.gif') repeat-x left top; */
  cursor: pointer;
  padding: 8px 10px;
  font: 13px/1em Verdana, Arial, Tahome;
  color: #343434;
  font-weight: bold;
}
.ac-box {
  display: block;
  background: #fcfcfc !important;
  border: 1px solid #e7e7e7;
  flex-direction: row;
  width: 1210px;
}
.fr {
  float: right;
  position: relative;
  bottom: 5px;
}
.fr-with-search {
  float: right;
  position: relative;
  bottom: 5px;
}
.prb-4 {
  position: relative;
  bottom: 4px;
}
/* section end */
/* span[aria-expanded="false"]  >img {
  transform: rotate(180deg);
} */
div[aria-expanded="false"] > span.fr > img {
  transform: rotate(180deg);
}
.body-container {
  background-color: #ffffff;
  min-height: 95vh;
}
.h-100 {
  height: 100%;
}
.rightside-dropdown-containers {
  display: flex;
  flex-direction: column;
  height: 100%;
  justify-content: start;
}
