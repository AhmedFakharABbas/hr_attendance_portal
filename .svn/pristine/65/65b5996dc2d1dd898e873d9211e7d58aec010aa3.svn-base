<div class="w-100">
  <div
    class="ac-left-box mb-15"
    *ngIf="!(isLRDatapatch || shiftChangeDataPatch)"
  >
    <form [formGroup]="attendanceCorrectionForm" #formDirective="ngForm">
      <div
        class="ac-title collapsed"
        data-toggle="collapse"
        data-target="#attendanceCorrection"
        aria-expanded="true"
      >
        Attendance Correction
        <span class="fr atend">
          <img src="../../../../../assets/icons/Up Arrow.svg"
        /></span>
      </div>
      <div class="collapse show box-shadow" id="attendanceCorrection">
        <div *ngIf="componentType.type != 'Emp-form'" class="d-flex">
          <p class="width-260 ml-10 mt-15">
            <mat-form-field class="width-260" appearance="outline">
              <mat-label>Employee Code </mat-label>
              <input
                formControlName="employee_code"
                matInput
                placeholder=""
                readonly
              />
            </mat-form-field>
            <span
              *ngIf="
                attendanceCorrectionForm.controls['employee_code'].invalid &&
                attendanceCorrectionForm.controls['employee_code'].touched
              "
            >
              <mat-error
                *ngIf="
                  attendanceCorrectionForm.controls['employee_code'].errors
                    ?.required
                "
                class="mat-error-msg"
                >Required*</mat-error
              >
            </span>
          </p>
          <p class="width-260 ml-10 mt-15">
            <mat-form-field
              class="width-260"
              appearance="outline"
              *ngIf="componentType.type != 'Emp-form'"
            >
              <mat-label>Employee Name </mat-label>
              <input
                formControlName="employee_name"
                matInput
                placeholder=""
                readonly
              />
            </mat-form-field>
            <span
              *ngIf="
                attendanceCorrectionForm.controls['employee_name'].invalid &&
                attendanceCorrectionForm.controls['employee_name'].touched
              "
            >
              <mat-error
                *ngIf="
                  attendanceCorrectionForm.controls['employee_name'].errors
                    ?.required
                "
                class="mat-error-msg"
                >Required*</mat-error
              >
            </span>
          </p>
          <p class="width-260 ml-10 mt-15"
        >
          <mat-form-field
            class="width-260 pointer"
            appearance="outline"
            (click)="
              this.componentType.type == 'Emp-form' && !isAttendancDatapatch
                ? rosterDateChangePicker.open()
                : ''
            "
          >
            <mat-label>Date</mat-label>

            <input
              (dateChange)="onRosterDateChange()"
              formControlName="roster_date"
              [max]="today"
              matInput
              [matDatepicker]="rosterDateChangePicker"
              [readonly]="
                componentType.type == 'hr-form' ||
                componentType.type == 'lm-form' ||
                isAttendancDatapatch
              "
            />

            <mat-icon matSuffix class="icon-align">
              <img
                src="../../../../../assets/icons/calendar.svg"
                class="font-image"
              />
            </mat-icon>
            <mat-datepicker #rosterDateChangePicker> </mat-datepicker>
          </mat-form-field>
          <span
            *ngIf="
              attendanceCorrectionForm.controls['roster_date'].invalid &&
              attendanceCorrectionForm.controls['roster_date'].touched
            "
          >
            <mat-error
              *ngIf="
                attendanceCorrectionForm.controls['roster_date'].errors
                  ?.required
              "
              class="mat-error-msg"
              >Required*</mat-error
            >
          </span>
        </p>
        </div>
        <div class="d-flex">
          <p
          *ngIf="componentType.type == 'Emp-form'"
            [ngClass]="componentType.type == 'Emp-form' ? 'mt-15' : ''"
            class="width-260 ml-10"

          >
            <mat-form-field
              class="width-260 pointer"
              appearance="outline"
              (click)="
                this.componentType.type == 'Emp-form' && !isAttendancDatapatch
                  ? rosterDateChangePicker.open()
                  : ''
              "
            >
              <mat-label>Date</mat-label>

              <input
                (dateChange)="onRosterDateChange()"
                formControlName="roster_date"
                [max]="today"
                matInput
                [matDatepicker]="rosterDateChangePicker"
                [readonly]="
                  componentType.type == 'hr-form' ||
                  componentType.type == 'lm-form' ||
                  isAttendancDatapatch
                "
              />

              <mat-icon matSuffix class="icon-align">
                <img
                  src="../../../../../assets/icons/calendar.svg"
                  class="font-image"
                />
              </mat-icon>
              <mat-datepicker #rosterDateChangePicker> </mat-datepicker>
            </mat-form-field>
            <span
              *ngIf="
                attendanceCorrectionForm.controls['roster_date'].invalid &&
                attendanceCorrectionForm.controls['roster_date'].touched
              "
            >
              <mat-error
                *ngIf="
                  attendanceCorrectionForm.controls['roster_date'].errors
                    ?.required
                "
                class="mat-error-msg"
                >Required*</mat-error
              >
            </span>
          </p>
          <p
            [ngClass]="componentType.type == 'Emp-form' ? 'mt-15' : ''"
            class="width-260 ml-10"
          >
            <mat-form-field class="width-260 pointer" appearance="outline">
              <mat-label>Time In </mat-label>
              <input
                matInput
                formControlName="auto_fill_time_in"
                [ngxTimepicker]="autoFillTimeIn"
                [disableClick]="true"
                readonly
              />
              <mat-icon matSuffix class="icon-align"
                ><img
                  src="../../../../../assets/icons/Time.svg"
                  class="font-image"
              /></mat-icon>
            </mat-form-field>

            <ngx-material-timepicker #autoFillTimeIn></ngx-material-timepicker>
            <span
              *ngIf="
                attendanceCorrectionForm.controls['auto_fill_time_in']
                  .invalid &&
                attendanceCorrectionForm.controls['auto_fill_time_in'].touched
              "
            >
              <mat-error
                *ngIf="
                  attendanceCorrectionForm.controls['auto_fill_time_in'].errors
                    ?.required
                "
                class="mat-error-msg"
                >Required*</mat-error
              >
            </span>
          </p>
          <p
            [ngClass]="componentType.type == 'Emp-form' ? 'mt-15 mr-10' : ''"

            class="width-260 ml-10 "
          >
            <mat-form-field class="width-260 pointer" appearance="outline">
              <mat-label>Time Out </mat-label>
              <input
                matInput
                formControlName="auto_fill_time_out"
                [ngxTimepicker]="autoFillTimeOut"
                [disableClick]="true"
                readonly
              />
              <mat-icon matSuffix class="icon-align"
                ><img
                  src="../../../../../assets/icons/Time.svg"
                  class="font-image"
              /></mat-icon>
            </mat-form-field>

            <ngx-material-timepicker #autoFillTimeOut></ngx-material-timepicker>
            <span
              *ngIf="
                attendanceCorrectionForm.controls['auto_fill_time_out']
                  .invalid &&
                attendanceCorrectionForm.controls['auto_fill_time_out'].touched
              "
            >
              <mat-error
                *ngIf="
                  attendanceCorrectionForm.controls['auto_fill_time_out'].errors
                    ?.required
                "
                class="mat-error-msg"
                >Required*</mat-error
              >
            </span>
          </p>
          <p   class="width-260 ml-10 mr-10" *ngIf="componentType.type != 'Emp-form'">
            <mat-form-field class="w-100" appearance="outline">
              <mat-label> Brief Reason</mat-label>
              <input
                [readonly]="
                  componentType.type == 'hr-form' ||
                  componentType.type == 'lm-form' ||
                  (isAttendancDatapatch && !resubmitAttendanceCorrection)
                "
                formControlName="reason"
                matInput
                placeholder=""
              />
            </mat-form-field>
            <span
              *ngIf="
                attendanceCorrectionForm.controls['reason'].invalid &&
                attendanceCorrectionForm.controls['reason'].touched
              "
            >
              <mat-error
                *ngIf="
                  attendanceCorrectionForm.controls['reason'].errors?.required
                "
                class="mat-error-msg"
                >Required*
              </mat-error>
            </span>
          </p>
        </div>

        <div
          *ngIf="componentType.type == 'Emp-form'"
          class="d-flex"
          style="width: 540px"
        >
          <p class="w-100 ml-10" *ngIf="this.lmData == undefined">
            <mat-form-field
              class="width-260 pointer"
              appearance="outline"
              (click)="componentType.type == 'Emp-form' ? timeIn.open() : ''"
            >
              <mat-label>Time Correction In</mat-label>
              <input
                matInput
                formControlName="time_in"
                [ngxTimepicker]="timeIn"
                [readonly]="
                  componentType.type == 'hr-form' ||
                  componentType.type == 'lm-form' ||
                  isAttendancDatapatch
                "
              />
              <mat-icon matSuffix class="icon-align"
                ><img
                  src="../../../../../assets/icons/Time.svg"
                  class="font-image"
              /></mat-icon>
            </mat-form-field>

            <ngx-material-timepicker #timeIn></ngx-material-timepicker>
            <span
              *ngIf="
                attendanceCorrectionForm.controls['time_in'].invalid &&
                attendanceCorrectionForm.controls['time_in'].touched
              "
            >
              <mat-error
                *ngIf="
                  attendanceCorrectionForm.controls['time_in'].errors?.required
                "
                class="mat-error-msg"
                >Required*</mat-error
              >
            </span>
          </p>
          <p class="w-100 ml-10" *ngIf="this.lmData == undefined">
            <mat-form-field
              class="width-260 pointer"
              appearance="outline"
              (click)="componentType.type == 'Emp-form' ? timeOut.open() : ''"
            >
              <mat-label>Time Correction Out </mat-label>
              <input
                matInput
                formControlName="time_out"
                [ngxTimepicker]="timeOut"
                [readonly]="
                  componentType.type == 'hr-form' ||
                  componentType.type == 'lm-form' ||
                  isAttendancDatapatch
                "
              />
              <mat-icon matSuffix class="icon-align"
                ><img
                  src="../../../../../assets/icons/Time.svg"
                  class="font-image"
              /></mat-icon>
            </mat-form-field>

            <ngx-material-timepicker #timeOut></ngx-material-timepicker>
            <span
              *ngIf="
                attendanceCorrectionForm.controls['time_out'].invalid &&
                attendanceCorrectionForm.controls['time_out'].touched
              "
            >
              <mat-error
                *ngIf="
                  attendanceCorrectionForm.controls['time_out'].errors?.required
                "
                class="mat-error-msg"
                >Required*</mat-error
              >
              <mat-error
                *ngIf="
                  attendanceCorrectionForm.controls['time_out'].errors?.timeOut
                "
                class="mat-error-msg"
                >The time out must be after time in.
              </mat-error>
            </span>
          </p>
        </div>
        <div class="d-flex" *ngIf="componentType.type == 'Emp-form'">
          <p class="w-100 ml-10 mr-10">
            <mat-form-field class="w-100" appearance="outline">
              <mat-label> Brief Reason</mat-label>
              <input
                [readonly]="
                  componentType.type == 'hr-form' ||
                  componentType.type == 'lm-form' ||
                  (isAttendancDatapatch && !resubmitAttendanceCorrection)
                "
                formControlName="reason"
                matInput
                placeholder=""
              />
            </mat-form-field>
            <span
              *ngIf="
                attendanceCorrectionForm.controls['reason'].invalid &&
                attendanceCorrectionForm.controls['reason'].touched
              "
            >
              <mat-error
                *ngIf="
                  attendanceCorrectionForm.controls['reason'].errors?.required
                "
                class="mat-error-msg"
                >Required*
              </mat-error>
            </span>
          </p>
        </div>

        <!-- Line Mangager  HR section   for LM/HR    -->
        <div class="lm-hr" *ngFor="let lm of lmData; let i = index">
          <div class="lm-box">
            <div
              class="lm-title"
              data-toggle="collapse"
              [attr.data-target]="'#LmCorrection' + i"
              aria-expanded="true"
            >
              {{
                (lm.role == "LM" ? "Line Manager " : "HR ") +
                  lm.count +
                  "" +
                  " (" +
                  lm.name +
                  " )"
              }}
              <span class="fr atend">
                <img src="../../../../../assets/icons/Up Arrow.svg"
              /></span>
            </div>
            <div class="collapse show" id="{{ 'LmCorrection' + i }}">
              <div class="d-flex">
                <p class="width-250 ml-10 mt-15">
                  <mat-form-field
                    class="width-250 pointer"
                    appearance="outline"
                  >
                    <mat-label>Date</mat-label>
                    <input
                      formControlName="lm_date"
                      [max]="today"
                      matInput
                      [matDatepicker]="lmPicker"
                      readonly
                    />
                    <mat-icon matSuffix class="icon-align"
                      ><img
                        src="../../../../../assets/icons/calendar.svg"
                        class="font-image"
                    /></mat-icon>
                    <mat-datepicker #lmPicker></mat-datepicker>
                  </mat-form-field>
                </p>
                <p class="width-250 ml-10 mt-15">
                  <mat-form-field
                    class="width-250 pointer"
                    appearance="outline"
                  >
                    <mat-label>Time In</mat-label>
                    <input
                      matInput
                      [formControlName]="'lm_time_in_' + i"
                      readonly
                    />

                    <mat-icon matSuffix class="icon-align"
                      ><img
                        src="../../../../../assets/icons/Time.svg"
                        class="font-image"
                    /></mat-icon>
                  </mat-form-field>

                  <ngx-material-timepicker #lmTimeIn></ngx-material-timepicker>
                </p>
                <p class="width-250 ml-10 mr-10 mt-15">
                  <mat-form-field
                    class="width-250 pointer"
                    appearance="outline"
                  >
                    <mat-label>Time Out</mat-label>
                    <input
                      matInput
                      [formControlName]="'lm_time_out_' + i"
                      readonly
                    />
                    <mat-icon matSuffix class="icon-align"
                      ><img
                        src="../../../../../assets/icons/Time.svg"
                        class="font-image"
                    /></mat-icon>
                  </mat-form-field>

                  <ngx-material-timepicker #lmTimeOut></ngx-material-timepicker>
                </p>
              </div>

              <div class="d-flex">
                <p class="width-770 ml-10 mr-10">
                  <mat-form-field class="width-770" appearance="outline">
                    <mat-label>Remarks</mat-label>
                    <input
                      readonly
                      [formControlName]="'lm_comments_' + i"
                      matInput
                      placeholder=""
                    />
                  </mat-form-field>
                </p>
              </div>
            </div>
          </div>
        </div>
        <!-- Line Manager section -->

        <div
          class="lm-box"
          *ngIf="
            (this.componentType.type == 'lm-form' &&
              isLmHrActionDisable == false) ||
            (this.componentType.type == 'hr-form' && this.lmData == undefined)
          "
        >
          <div
            class="lm-title"
            data-toggle="collapse"
            data-target="#lmInputForm"
            aria-expanded="true"
          >
            Line Manager ({{ employeeName }})
            <span class="fr atend">
              <img src="../../../../../assets/icons/Up Arrow.svg"
            /></span>
          </div>
          <div class="collapse show" id="lmInputForm">
            <div
              *ngIf="
                (this.componentType.type == 'lm-form' &&
                  isLmHrActionDisable == false) ||
                (this.componentType.type == 'hr-form' &&
                  this.lmData == undefined)
              "
              class="d-flex"
            >
              <p
                [ngClass]="lmData != undefined ? 'mt-15' : ''"
                class="width-250 ml-10 mt-15"
              >
                <mat-form-field class="width-250 pointer" appearance="outline">
                  <mat-label>Date</mat-label>
                  <input
                    formControlName="lm_date"
                    [max]="today"
                    matInput
                    [matDatepicker]="lmPicker"
                    readonly
                  />
                  <mat-icon matSuffix class="icon-align"
                    ><img
                      src="../../../../../assets/icons/calendar.svg"
                      class="font-image"
                  /></mat-icon>
                  <mat-datepicker #lmPicker></mat-datepicker>
                </mat-form-field>
                <span
                  *ngIf="
                    attendanceCorrectionForm.controls['lm_date'].invalid &&
                    attendanceCorrectionForm.controls['lm_date'].touched
                  "
                >
                  <mat-error
                    *ngIf="
                      attendanceCorrectionForm.controls['lm_date'].errors
                        ?.required
                    "
                    class="mat-error-msg"
                    >Required*</mat-error
                  >
                </span>
              </p>
              <p
                [ngClass]="lmData != undefined ? 'mt-15' : ''"
                class="width-250 ml-10 mt-15"
              >
                <mat-form-field
                  class="width-250 pointer"
                  appearance="outline"
                  (click)="
                    componentType.type == 'lm-form' ? lmTimeIn.open() : ''
                  "
                >
                  <mat-label>Time In</mat-label>
                  <input
                    matInput
                    formControlName="lm_time_in"
                    [ngxTimepicker]="lmTimeIn"
                    [disableClick]="componentType.type != 'lm-form'"
                    [readonly]="componentType.type != 'lm-form'"
                  />
                  <mat-icon matSuffix class="icon-align"
                    ><img
                      src="../../../../../assets/icons/Time.svg"
                      class="font-image"
                  /></mat-icon>
                </mat-form-field>

                <ngx-material-timepicker #lmTimeIn></ngx-material-timepicker>
                <span
                  *ngIf="
                    attendanceCorrectionForm.controls['lm_time_in'].invalid &&
                    attendanceCorrectionForm.controls['lm_time_in'].touched
                  "
                >
                  <mat-error
                    *ngIf="
                      attendanceCorrectionForm.controls['lm_time_in'].errors
                        ?.required
                    "
                    class="mat-error-msg"
                    >Required*</mat-error
                  >
                </span>
              </p>
              <p
                [ngClass]="lmData != undefined ? 'mt-15' : ''"
                class="width-250 ml-10 mt-15"
              >
                <mat-form-field
                  class="width-250 pointer"
                  appearance="outline"
                  (click)="
                    componentType.type == 'lm-form' ? lmTimeOut.open() : ''
                  "
                >
                  <mat-label>Time Out</mat-label>
                  <input
                    matInput
                    formControlName="lm_time_out"
                    [ngxTimepicker]="lmTimeOut"
                    [disableClick]="componentType.type != 'lm-form'"
                    [readonly]="componentType.type != 'lm-form'"
                  />
                  <mat-icon matSuffix class="icon-align"
                    ><img
                      src="../../../../../assets/icons/Time.svg"
                      class="font-image"
                  /></mat-icon>
                </mat-form-field>

                <ngx-material-timepicker #lmTimeOut></ngx-material-timepicker>
                <span
                  *ngIf="
                    attendanceCorrectionForm.controls['lm_time_out'].invalid &&
                    attendanceCorrectionForm.controls['lm_time_out'].touched
                  "
                >
                  <mat-error
                    *ngIf="
                      attendanceCorrectionForm.controls['lm_time_out'].errors
                        ?.required
                    "
                    class="mat-error-msg"
                    >Required*</mat-error
                  >
                  <mat-error
                    *ngIf="
                      attendanceCorrectionForm.controls['lm_time_out'].errors
                        ?.LMtimeOut
                    "
                    class="mat-error-msg"
                    >The time out must be after time in.
                  </mat-error>
                </span>
              </p>
            </div>
            <div
              *ngIf="
                (this.componentType.type == 'lm-form' &&
                  isLmHrActionDisable == false) ||
                (this.componentType.type == 'hr-form' &&
                  this.lmData == undefined)
              "
              class="d-flex"
            >
              <p class="w-100 ml-10 mr-10">
                <mat-form-field class="w-100" appearance="outline">
                  <mat-label>Remarks</mat-label>

                  <input
                    [readonly]="componentType.type != 'lm-form'"
                    formControlName="lm_comments"
                    matInput
                    placeholder=""
                  />
                </mat-form-field>
              </p>
            </div>
          </div>
        </div>
        <!-- Line Manager section end -->
        <!-- HR Section start -->

        <div
          class="lm-box"
          *ngIf="
            componentType.type == 'hr-form' && isLmHrActionDisable == false
          "
        >
          <div
            class="lm-title"
            data-toggle="collapse"
            data-target="#hrInputForm"
            aria-expanded="true"
          >
            HR ({{ employeeName }})
            <span class="fr atend">
              <img src="../../../../../assets/icons/Up Arrow.svg"
            /></span>
          </div>
          <div class="collapse show" id="hrInputForm">
            <div
              *ngIf="
                componentType.type == 'hr-form' && isLmHrActionDisable == false
              "
              class="d-flex"
            >
              <p class="width-250 ml-10 mt-15">
                <mat-form-field class="width-250 pointer" appearance="outline">
                  <mat-label>Date</mat-label>
                  <input
                    formControlName="hr_date"
                    [max]="today"
                    matInput
                    [matDatepicker]="hrDpicker"
                  />
                  <mat-icon matSuffix class="icon-align"
                    ><img
                      src="../../../../../assets/icons/calendar.svg"
                      class="font-image"
                  /></mat-icon>
                  <mat-datepicker #hrDpicker></mat-datepicker>
                </mat-form-field>
                <span
                  *ngIf="
                    attendanceCorrectionForm.controls['hr_date'].invalid &&
                    attendanceCorrectionForm.controls['hr_date'].touched
                  "
                >
                  <mat-error
                    *ngIf="
                      attendanceCorrectionForm.controls['hr_date'].errors
                        ?.required
                    "
                    class="mat-error-msg"
                    >Required*</mat-error
                  >
                </span>
              </p>
              <p class="width-250 ml-10 mt-15">
                <mat-form-field
                  class="width-250 pointer"
                  appearance="outline"
                  (click)="
                    componentType.type == 'hr-form' ? hrTimeIn.open() : ''
                  "
                >
                  <mat-label>Time In</mat-label>
                  <input
                    matInput
                    formControlName="hr_time_in"
                    [ngxTimepicker]="hrTimeIn"
                  />
                  <mat-icon matSuffix class="icon-align"
                    ><img
                      src="../../../../../assets/icons/Time.svg"
                      class="font-image"
                  /></mat-icon>
                </mat-form-field>

                <ngx-material-timepicker #hrTimeIn></ngx-material-timepicker>
                <span
                  *ngIf="
                    attendanceCorrectionForm.controls['hr_time_in'].invalid &&
                    attendanceCorrectionForm.controls['hr_time_in'].touched
                  "
                >
                  <mat-error
                    *ngIf="
                      attendanceCorrectionForm.controls['hr_time_in'].errors
                        ?.required
                    "
                    class="mat-error-msg"
                    >Required*</mat-error
                  >
                </span>
              </p>
              <p class="width-250 ml-10 mt-15">
                <mat-form-field
                  class="width-250 pointer"
                  appearance="outline"
                  (click)="
                    componentType.type == 'hr-form' ? hrTimeOut.open() : ''
                  "
                >
                  <mat-label>Time Out</mat-label>
                  <input
                    matInput
                    formControlName="hr_time_out"
                    [ngxTimepicker]="hrTimeOut"
                  />
                  <mat-icon matSuffix class="icon-align"
                    ><img
                      src="../../../../../assets/icons/Time.svg"
                      class="font-image"
                  /></mat-icon>
                </mat-form-field>

                <ngx-material-timepicker #hrTimeOut></ngx-material-timepicker>
                <span
                  *ngIf="
                    attendanceCorrectionForm.controls['hr_time_out'].invalid &&
                    attendanceCorrectionForm.controls['hr_time_out'].touched
                  "
                >
                  <mat-error
                    *ngIf="
                      attendanceCorrectionForm.controls['hr_time_out'].errors
                        ?.required
                    "
                    class="mat-error-msg"
                    >Required*</mat-error
                  >
                  <mat-error
                    *ngIf="
                      attendanceCorrectionForm.controls['hr_time_out'].errors
                        ?.HRtimeOut
                    "
                    class="mat-error-msg"
                    >The time out must be after time in.</mat-error
                  >
                </span>
              </p>
            </div>

            <div
              *ngIf="
                componentType.type == 'hr-form' && isLmHrActionDisable == false
              "
              class="d-flex"
            >
              <p class="w-100 ml-10 mr-10">
                <mat-form-field class="w-100" appearance="outline">
                  <mat-label>Remarks</mat-label>
                  <input
                    formControlName="hr_comments"
                    matInput
                    placeholder=""
                  />
                </mat-form-field>
                <span
                  *ngIf="
                    attendanceCorrectionForm.controls['hr_comments'].invalid &&
                    attendanceCorrectionForm.controls['hr_comments'].touched
                  "
                >
                  <mat-error
                    *ngIf="
                      attendanceCorrectionForm.controls['hr_comments'].errors
                        ?.required
                    "
                    class="mat-error-msg"
                    >Required*</mat-error
                  >
                  <mat-error
                    *ngIf="
                      !attendanceCorrectionForm.controls['hr_comments'].errors
                        ?.required &&
                      attendanceCorrectionForm.controls['hr_comments'].errors
                        ?.minlength
                    "
                    class="mat-error-msg"
                    >Remarks must be at least 10 characters long.
                  </mat-error>
                </span>
              </p>
            </div>
          </div>
        </div>

        <!-- HR section End -->

        <div class="ac-footer">
          <div class="text-right ac-button-list">
            <button
              (click)="onApproveStatus(2, formDirective)"
              *ngIf="componentType.type != 'Emp-form'"
              [disabled]="isLmHrActionDisable"
              type="button"
              class="submitButton ml-1"
            >
              Approve
            </button>
            <button
              (click)="onApproveStatus(3, formDirective)"
              [disabled]="isLmHrActionDisable"
              *ngIf="componentType.type != 'Emp-form'"
              type="button"
              class="submitButton ml-1"
            >
              Disapprove
            </button>
            <button
              *ngIf="
                componentType.type == 'Emp-form' && resubmitAttendanceCorrection
              "
              type="button"
              (click)="onReSubmitAttendaceCorrection(formDirective)"
              class="submitButton ml-1"
            >
              Resubmit
            </button>
            <button
              *ngIf="
                componentType.type == 'Emp-form' &&
                !resubmitAttendanceCorrection
              "
              [disabled]="isAttendancDatapatch"
              type="button"
              (click)="onSubmitAttendaceCorrection(formDirective)"
              class="submitButton ml-1"
            >
              Submit
            </button>
            <button
              type="button"
              (click)="
                this.attendanceCorrectionForm.reset();
                this.attendanceCorrection();
                this.lmData = undefined;
                this.isLmHrActionDisable = false;
                this.resubmitAttendanceCorrection = false;
                this.isAttendancDatapatch = false;
                this.isLmHrActionDisable = false;
                this.shiftChangeDataPatch = false;
                this.isLRDatapatch = false;
                this.rowData = undefined
              "
              class="submitButton ml-1"
            >
              Cancel
            </button>
          </div>
        </div>
      </div>
    </form>
  </div>
</div>

<!-- Leave Reversal -->
