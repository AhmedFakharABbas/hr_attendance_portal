              <mat-label>Employee Code </mat-label>
              <input
                formControlName="employee_code"
                matInput
                placeholder=""
                readonly
              />
            </mat-form-field>
            <span
              *ngIf="
                leaveReversalForm.controls['employee_code'].invalid &&
                leaveReversalForm.controls['employee_code'].touched
              "
            >
              <mat-error
                *ngIf="
                  leaveReversalForm.controls['employee_code'].errors?.required
