<div class="body-container">
  <div class="ac-form-container">
    <div class="ac-left-container">
      <app-attendance-form [componentType]="empForm"></app-attendance-form>
    </div>

    <!-- Correction Status -->
    <div>
      <div class="ac-right-container">
        <div class="w-100">
          <div class="ac-right-box mt-2">
            <div
              class="ac-title"
              data-toggle="collapse"
              data-target="#correctionStatus"
              aria-expanded="true"
            >
              Correction Status
              <span class="fr"
                ><img src="../../../../../assets/icons/Up Arrow.svg"
              /></span>
            </div>
            <div class="collapse show" id="correctionStatus">
              <app-attendance-table
                [componentType]="correctionStatus"
              ></app-attendance-table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- Correction Applicaitons -->
  <div class="ac-container">
    <div class="w-100">
      <div class="ac-box ml-2 mt-2">
        <div
          class="ac-title"
          data-toggle="collapse"
          data-target="#correctionApplicaitons"
          aria-expanded="true"
        >
          Correction Applicaitons
          <span class="fr"
            ><img src="../../../../../assets/icons/Up Arrow.svg"
          /></span>
        </div>
        <div class="ac-content">
          <!-- table  start-->
          <div class="collapse show" id="correctionApplicaitons">
            <app-attendance-table
              [componentType]="correctionApplication"
            ></app-attendance-table>
          </div>
          <!-- table end-->
        </div>
      </div>
    </div>
  </div>
</div>
