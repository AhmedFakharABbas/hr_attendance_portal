#ssnackbar {
  visibility: hidden;
  min-width: 250px;
  max-width: 350px;
  margin-left: -125px;
  background-color: #61bf63;
  color: #fff;
  /* text-align: center; */
  border-radius: 2px;
  padding: 15px;
  font-size: 15px;
  position: fixed;
  z-index: 1;
  left: 50%;
  bottom: 30px;
  cursor: pointer;
}
.crossIcon {
  float: right;
  padding-left: 15px;
}
#ssnackbar.show {
  visibility: visible;
  -webkit-animation: fadein 0.5s, fadeout 0.5s 4.5s;
  animation: fadein 0.5s, fadeout 0.5s 4.5s;
}
