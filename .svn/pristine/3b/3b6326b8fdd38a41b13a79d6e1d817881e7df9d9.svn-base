/* ng slect guideline */
.ng-select .ng-select-container {
  cursor: default;
  display: -webkit-box;
  display: flex;
  outline: 0;
  overflow: visible !important;
  position: relative;
  width: 100%;
  border-radius: 5px;
  background: #486c9154;
}

.ng-select.ng-select-opened.ng-select-bottom > .ng-select-container {
  /* border-bottom-right-radius: 39px; */
  /* border-bottom-left-radius: 0; */
  border-radius: 5px;
  border: 0.13rem solid #ba0143;
}
.ng-select .ng-has-value .ng-placeholder,
.ng-select.ng-select-opened .ng-placeholder {
  color: #ba0143;
  font-size: 14px !important;
}
.ng-select.ng-select-focused
  .ng-select-container
  .ng-value-container
  .ng-placeholder {
  -webkit-transform: translateY(-1.35em) scale(0.75) perspective(100px)
    translateZ(0.001px);
  transform: translateY(-1.35em) scale(0.75) perspective(100px)
    translateZ(0.001px);
  color: #ba0143;
  box-shadow: none !important;
}

.ng-select .ng-has-value .ng-placeholder,
.ng-select.ng-select-opened .ng-placeholder {
  -webkit-transform: translateY(-1.35em) scale(0.75) perspective(100px)
    translateZ(0.001px);
  transform: translateY(-1.35em) scale(0.75) perspective(100px)
    translateZ(0.001px);
  color: lightgray;
  z-index: 0 !important;
}
.floating-label {
  position: relative;
}
.on_Hold > .ng-select-container {
  background: #ba0143 !important;
}

.Open > .ng-select-container {
  background: #fb5347 !important;
}

.Complete > .ng-select-container {
  background: #878c90 !important;
}
