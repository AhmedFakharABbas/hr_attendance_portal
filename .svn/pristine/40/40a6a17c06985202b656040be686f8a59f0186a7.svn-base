/* -------------------- Slider Buttons */

.leftRsBanner:focus,
.rightRsBanner:focus {
    outline: none;
}

.leftRsBanner,
.rightRsBanner {
    position: absolute;
    top: 50%;
    margin-top: -22px;
    width: 30px;
    height: 30px;
    background: 0 0 !important;
    cursor: pointer
}

.leftRsBanner::before {
    font-family: FontAwesome;
    content: '';
    font-size: 20px;
    line-height: 36px;
    color: #fff;
    display: inline-block;
    vertical-align: middle;
    -webkit-transition: .5s all;
    -o-transition: .5s all;
    -moz-transition: .5s all;
    transition: .5s all
}

.leftRsBanner:hover,
.rightRsBanner:hover {
    background-color: #a9a8a8 !important;
    color: #fff;
}

.leftRsBanner {
    position: absolute;
    left: 2px;
    top: 55%;
    border-radius: 100%;
    background: rgba(0, 0, 0, 0.7490196078431373);
    border: none;
    font-size: 14px;
    padding: 5px 10px;
    color: #a9a8a8;
    z-index: 10;
}

.rightRsBanner {
    position: absolute;
    right: 2px;
    top: 55%;
    border-radius: 100%;
    background: rgba(0, 0, 0, 0.7490196078431373);
    border: none;
    font-size: 14px;
    padding: 5px 10px;
    color: #a9a8a8;
}

.leftRsBanner::before {
    line-height: 20px
}


/* ::ng-deep .leftRsBanner,
::ng-deep .rightRsBanner {
  width: 30px;
  height: 30px
} */

.rightRsBanner:before {
    line-height: 25px
}

.slick-slide:focus {
    outline: 0;
}

.people-title-sub-heading {
    background-color: #f1f1f1;
    padding: 5px 12px;
    font: 12px Tahoma, Arial, Verdana;
    font-weight: 600;
    height: 23px;
}

.portal-container {
