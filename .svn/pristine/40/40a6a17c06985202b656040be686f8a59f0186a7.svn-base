/* ================ Dropdown */

.btn-group li {
    width: 100%;
}

.btn-group .label-btn {
    width: 117px;
    padding: 2px 5px;
}

.btn-group ul {
    background: #fff;
    padding: 0;
}

.btn-white:focus {
    box-shadow: none;
}

.btn-white {
    background-color: #f7f7f7;
    border: 1px solid #dcdcdc;
    border-radius: 0;
    text-align: left;
}

.btn-group ul li a {
    cursor: pointer;
    padding: 5px 10px;
    border-bottom: 1px solid #dcdcdc;
}

.dropdown-item:hover {
    background-color: #eee;
}

.btn-group button,
.btn-group ul li a {
    font: 12px Tahoma, Arial, Verdana;
}

.dropdown-menu {
    min-width: 7.4rem;
}

.disabledDiv {
    pointer-events: none;
    opacity: 0.6;
}

.caret-img {
    background-color: #f7f7f7;
    border: none;
    border-left: 1px solid #dcdcdc;
    border-top-right-radius: 0px;
    border-bottom-right-radius: 0px;
    position: absolute;
    top: 1px;
    right: 1px;
    width: 17px;
    height: 17px;
    background-image: url(../images/select-btn.gif);
    background-repeat: no-repeat;
    background-position: 41% 41%;
}

.dropdown-menu-selected {
    color: #3378C7 !important;
}

.cdk-overlay-container {
    z-index: 1050;
}


/* HighChart custom CSS */

.highcharts-credits {
    display: none;
}

.highcharts-title {
    font: 14px Roboto, Helvetica Neue, Sans-Serif !important;
    font-weight: 500 !important;
}


.mgn-top{
  margin-top: 10px;
}

.btn-add-col{
  background-color: #ffffff;
  box-sizing: border-box;
  position: relative;
  user-select: none;
  cursor: pointer;
  outline: none;
  border: none;
  -webkit-tap-highlight-color: transparent;
  display: inline-block;
  white-space: nowrap;
  text-decoration: none;
  vertical-align: baseline;
  text-align: center;
  margin: 0;
  min-width: 62px;
  line-height: 28px;
  padding: 0 16px;
  border-radius: 4px;
  margin-top: 5px;
  font-size: 12px;
  border: 1px solid #1A73E8;
}

.btn-remove{
  border-radius: 4px;
  width: 30px;
  height: 20px;
  background-color: #1A73E8;
  text-decoration: none;
  position: relative;
  border: 1px solid #e6e6e6;
  color: #ffffff;
  margin-top: 7px;
  margin-right: 10px;
  margin-left: 5px;
}

.mat-header-cell {
  background: white !important
}

.mat-form-field-appearance-outline .mat-form-field-outline-thick{

  color: #1A73E8 !important;
}