}

textarea {
  border: none;
}
textarea:focus-visible {
  outline: none;
}
.td-w{
  width: 89vw;
    overflow-y: auto;
}
.submitButton {
  color: #3080ea;
  position: relative;
  padding: 2px 6px 2px 6px;
  /* border: px solid var(--unnamed-color-ffffff);  */
  background: transparent linear-gradient(180deg, #ededed 0%, #fafafa 100%) 0%
    0% no-repeat padding-box;
  border: 0px solid #ffffff;
  border-radius: 3px;

  opacity: 1;
  text-decoration: none;
  font-size: 11px;
  cursor: pointer;
  margin-left: 6px;
}
.submitButton:hover {
  /* border: 1px solid #a6daee; */
  padding: 2px 6px 2px 6px;
  background: transparent linear-gradient(0deg, #f1fafe 0%, #bde9fb 100%) 0% 0%
    no-repeat padding-box;
  border-radius: 3px;
  opacity: 1;
  color: #343434;
  text-decoration: none;
  font-size: 11px;
}
.heading {
  font-size: 14px;
  font-weight: 600;
  margin-top: 2px;
  margin-bottom: 2px;
}

.io-textArea {
  background-color: white;
  border: none !important ;
